{
  "name": "ercodingtheme",
  "version": "1.0.0",
  "resolutions": {
    "minimatch": "^9.0.3"
  },
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build:style": "sass --load-path=node_modules --style=expanded style.scss style.css && postcss -r style.css",
    "build:style-editor": "sass --load-path=node_modules --style=expanded style-editor.scss style-editor.css && postcss -r style-editor.css",
    "build:scripts:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=development",
    "build:scripts:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production",
    "build-dev": "run-p build:style build:style-editor build:scripts:dev",
    "build": "run-p build:style build:style-editor build:scripts:prod",
    "watch": "chokidar \"**/*.scss\" \"js/src/**/*.js\" -i \"node_modules/**/*\" -c \"yarn build-dev\" --initial",
    "lint:css": "stylelint \"**/*.scss\"",
    "fix:css": "stylelint \"**/*.scss\" --fix",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "lint": "run-p \"lint:*\"",
    "format": "prettier --write .",
    "blocks:apply": "node js/tools/blocks-apply.js",
    "check": "prettier --check .",
    "update-browserslist": "npx browserslist@latest --update-db"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@prettier/plugin-php": "^0.24.0",
    "@wordpress/eslint-plugin": "^3.1.0",
    "babel-loader": "^8.0.6",
    "chokidar-cli": "^2.0.0",
    "cross-env": "7.0.3",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^4.5.2",
    "gsap": "^3.11.4",
    "morphext": "^2.4.7",
    "parallax-js": "^3.1.0",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
